mixin navigate(id)
    .text-center
        a(class="btn btn-lg btn-primary btn-more",href="##{id}",data-id="#{id}")
            i(class="fa fa fa-arrow-down animated infinite fadeIn",aria-hidden="true")

mixin link(key)
  if nav[key]
    li.nav-item
      a.nav-link(href="##{key}",data-id="##{key}")|#{nav[key]}
