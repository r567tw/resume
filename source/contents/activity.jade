div(id="#{resume.data[5].id}")
    .jumbotron.wow.fadeIn
        h1 #{resume.data[5].name}
        hr(class="my-4")
        div.row
            each content,i in resume.data[5].contents
                .card.col-md-12
                    .card-block
                        h3(class="card-title")|#{content.title}
                        div(class="badge badge-lg badge-default")
                            |#{content.time}
                        p(class="card-text")
                            |#{content.description}